#notice= notice

This set was created by #{@parameter_set.user.email}

%p
  %b Name:
  = @parameter_set.name
%p
  %b Description:
  = @parameter_set.description
%p
  %b Brackets:
  = simple_format @parameter_set.brackets

.form-actions
  .btn= link_to t('.back', :default => t("helpers.links.back")), parameter_sets_path
  .btn= link_to t('.edit', :default => t("helpers.links.edit")), edit_parameter_set_path(@parameter_set)
  .btn.btn-danger= link_to t('.destroy', :default => t("helpers.links.destroy")), parameter_set_path(@parameter_set), :method => 'delete', :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?'))