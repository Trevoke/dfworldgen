<h1>Listing seeds</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Brackets</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @seeds.each do |seed| %>
  <tr>
    <td><%= seed.name %></td>
    <td><%= seed.description %></td>
    <td><%= seed.brackets %></td>
    <td><%= link_to 'Show', seed %></td>
    <td><%= link_to 'Edit', edit_seed_path(seed) %></td>
    <td><%= link_to 'Destroy', seed, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Seed', new_seed_path %>
